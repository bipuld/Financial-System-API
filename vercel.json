{
  "version": 2,
  "builds": [
    {
      "src": "FinanceFlow/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "FinanceFlow/wsgi.py"
    }
  ],
  "postinstall": "python manage.py migrate && python manage.py collectstatic --noinput"
}
